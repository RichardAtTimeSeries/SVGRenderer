<?xml version="1.0" encoding="utf-8" ?>
<widget id="SVGRenderer.widget.SVGRenderer" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>SVGRenderer</name>
    <description>The description of this widget.</description>

    <icon></icon>

    <properties>
        <property key="initialEntityToUse" type="entity" allowNonPersistableEntities="true">
            <caption>Entity to use</caption>
            <category>Initial state</category>
            <description>The entity to use to populate the SVG.</description>
        </property>
        <property key="initialSVGContent" type="attribute" entityProperty="initialEntityToUse">
            <caption>SVG Content</caption>
            <category>Initial state</category>
            <description>The SVG to render.</description>
             <attributeTypes>
               <attributeType name="String" />
             </attributeTypes>
        </property>
        <property key="initialStateNameToUse" type="attribute" entityProperty="initialEntityToUse">
            <caption>State</caption>
            <category>Initial state</category>
            <description>The state name this SVG is associated with.</description>
             <attributeTypes>
               <attributeType name="String" />
             </attributeTypes>
        </property>
        <property key="initialMicroflowToUse" type="microflow" required="false">
            <caption>Microflow</caption>
            <category>Initial state</category>
            <description>The Microflow to populate the SVG.</description>
            <returnType type="Object" entityProperty="initialEntityToUse"></returnType>
        </property>
    </properties>
</widget>

